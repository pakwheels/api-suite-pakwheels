{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "inspection_days"
  ],
  "properties": {
    "inspection_days": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "inspection_day_title",
          "inspection_date"
        ],
        "properties": {
          "inspection_day_title": {
            "type": "string"
          },
          "inspection_date": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "custom_dimensions": {
      "type": "object",
      "properties": {
        "user_type": {
          "type": "string"
        },
        "newsletter_subsriber": {
          "type": "boolean"
        },
        "car_listings": {
          "type": "string"
        },
        "bike_listings": {
          "type": "string"
        },
        "parts_listings": {
          "type": "string"
        },
        "private_rides": {
          "type": "string"
        },
        "public_rides": {
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
