{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "success",
    "error",
    "car_registration_transfer_lead_id",
    "user_id",
    "summary",
    "mobile",
    "mobile_verified",
    "mobile_uuid"
  ],
  "properties": {
    "success": { "type": "string" },
    "error": { "type": ["string", "null"] },
    "car_registration_transfer_lead_id": { "type": "integer" },
    "user_id": { "type": "integer" },
    "summary": {
      "type": "object",
      "required": ["car_info", "contact_info", "place_of_registration", "lead_type", "next_steps"],
      "properties": {
        "car_info": { "type": "string" },
        "contact_info": {
          "type": "object",
          "required": ["name", "mobile_number"],
          "properties": {
            "name": { "type": "string" },
            "mobile_number": { "type": "string" }
          },
          "additionalProperties": true
        },
        "place_of_registration": { "type": "string" },
        "lead_type": { "type": "string" },
        "next_steps": {
          "type": "array",
          "items": { "type": "string" }
        }
      },
      "additionalProperties": true
    },
    "mobile": { "type": "string" },
    "mobile_verified": { "type": "boolean" },
    "mobile_uuid": { "type": "string" }
  },
  "additionalProperties": true
}
