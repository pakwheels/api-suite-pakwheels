{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["carsure_cities", "error", "custom_dimensions"],
  "properties": {
    "carsure_cities": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "name", "position", "url_slug"],
        "properties": {
          "id": { "type": "integer" },
          "name": { "type": "string" },
          "position": { "type": "integer" },
          "url_slug": { "type": "string" }
        },
        "additionalProperties": true
      }
    },
    "error": { "type": "string" },
    "custom_dimensions": {
      "type": "object",
      "required": [
        "user_type",
        "newsletter_subsriber",
        "car_listings",
        "bike_listings",
        "parts_listings",
        "private_rides",
        "public_rides"
      ],
      "properties": {
        "user_type": { "type": "string" },
        "newsletter_subsriber": { "type": "boolean" },
        "car_listings": { "type": "string" },
        "bike_listings": { "type": "string" },
        "parts_listings": { "type": "string" },
        "private_rides": { "type": "string" },
        "public_rides": { "type": "string" }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
