      "items": {
        "type": "object",
        "required": ["city_id", "id", "name", "position", "url_slug", "zone"],
        "properties": {
          "city_id": { "type": "integer" },
          "id": { "type": "integer" },
          "name": { "type": "string" },
          "position": { "type": "integer" },
          "url_slug": { "type": "string" },
