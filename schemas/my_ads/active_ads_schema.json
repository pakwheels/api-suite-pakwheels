{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "pageNo",
    "totalPages",
    "resultCount",
    "ads"
  ],
  "properties": {
    "pageNo": { "type": "string" },
    "totalPages": { "type": "integer" },
    "resultCount": { "type": "integer" },
    "totalActiveAds": { "type": "integer" },
    "totalPendingAds": { "type": "integer" },
    "totalRemovedAds": { "type": "integer" },
    "totalAds": { "type": "integer" },
    "ads": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "id",
          "title",
          "status",
          "type",
          "city",
          "price",
          "isUserAd",
          "user"
        ],
        "properties": {
          "phone": { "type": ["string", "null"] },
          "description": { "type": ["string", "null"] },
          "urlSlug": { "type": "string" },
          "status": { "type": "integer" },
          "id": { "type": "integer" },
          "adListingId": { "type": "integer" },
          "type": { "type": "string" },
          "title": { "type": "string" },
          "city": { "type": "string" },
          "cityArea": { "type": ["string", "null"] },
          "price": { "type": ["integer", "number"] },
          "violations": { "type": "array" },
          "modelYear": { "type": ["integer", "null"] },
          "mileage": { "type": ["string", "null"] },
          "transmission": { "type": ["string", "null"] },
          "engineType": { "type": ["string", "null"] },
          "assembly": { "type": ["string", "null"] },
          "engineCapacity": { "type": ["string", "null"] },
          "isUserAd": { "type": "boolean" },
          "user": {
            "type": "object",
            "required": ["id", "displayName"],
            "properties": {
              "id": { "type": "integer" },
              "displayName": { "type": "string" },
              "mobileVerified": { "type": ["boolean", "null"] }
            }
          }
        }
      }
    },
    "custom_dimensions": { "type": "object" }
  }
}
