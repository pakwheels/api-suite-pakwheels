{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Toyota Corolla Model Detail",
  "type": "object",
  "required": [
    "heading",
    "page_type",
    "pictures",
    "make_id",
    "price_block",
    "model_name",
    "full_slug",
    "overview",
    "schema",
    "custom_dimensions"
  ],
  "properties": {
    "heading": {
      "type": "string"
    },
    "page_type": {
      "type": "object",
      "required": [
        "latest_generation",
        "upcoming_generation",
        "next_year_model",
        "old_generation"
      ],
      "properties": {
        "latest_generation": {
          "type": "boolean"
        },
        "upcoming_generation": {
          "type": "boolean"
        },
        "next_year_model": {
          "type": "boolean"
        },
        "old_generation": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    },
    "pictures": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "medium",
          "original",
          "thumbnail",
          "image_alt",
          "image_title"
        ],
        "properties": {
          "medium": {
            "type": "string",
            "format": "uri"
          },
          "original": {
            "type": "string",
            "format": "uri"
          },
          "thumbnail": {
            "type": "string",
            "format": "uri"
          },
          "image_alt": {
            "type": "string"
          },
          "image_title": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "model_id": {
      "type": "integer"
    },
    "make_id": {
      "type": "integer"
    },
    "price_block": {
      "type": "object",
      "required": [
        "price_rich_content",
        "data",
        "min_price",
        "max_price",
        "price_updated_at",
        "h2"
      ],
      "properties": {
        "price_rich_content": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "name",
              "id",
              "price",
              "link"
            ],
            "properties": {
              "generation_id": {
                "type": ["integer", "null"]
              },
              "name": {
                "type": "string"
              },
              "id": {
                "type": ["integer", "null"]
              },
              "price": {
                "type": "string"
              },
              "schema_price": {
                "type": ["integer", "number", "null"]
              },
              "link": {
                "type": "string"
              },
              "engine_capacity": {
                "type": ["string", "null"]
              },
              "transmission_type": {
                "type": ["string", "null"]
              },
              "engine_type": {
                "type": ["string", "null"]
              },
              "delivery_time": {
                "type": ["string", "null"]
              },
              "key_features": {
                "type": ["string", "null"]
              },
              "booking_amount": {
                "type": ["string", "null"]
              },
              "is_base_variant": {
                "type": ["boolean", "null"]
              },
              "is_top_variant": {
                "type": ["boolean", "null"]
              }
            },
            "additionalProperties": false
          }
        },
        "table_headers": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "asterisk_content": {
          "type": ["string", "null"]
        },
        "other_variants": {
          "type": "object"
        },
        "car_versions": {
          "type": "array"
        },
        "used_links": {
          "type": "array"
        },
        "min_price": {
          "type": ["integer", "number"]
        },
        "max_price": {
          "type": ["integer", "number"]
        },
        "price_updated_at": {
          "type": "string"
        },
        "h2": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "price": {
      "type": "string"
    },
    "model_name": {
      "type": "string"
    },
    "full_slug": {
      "type": "string"
    },
    "overview": {
      "type": "object",
      "required": [
        "h2",
        "description"
      ],
      "properties": {
        "h2": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "custom_dimensions": {
      "type": "object",
      "required": [
        "user_type",
        "newsletter_subsriber",
        "car_listings",
        "bike_listings",
        "parts_listings",
        "private_rides",
        "public_rides"
      ],
      "properties": {
        "user_type": {
          "type": "string"
        },
        "newsletter_subsriber": {
          "type": "boolean"
        },
        "car_listings": {
          "type": "string"
        },
        "bike_listings": {
          "type": "string"
        },
        "parts_listings": {
          "type": "string"
        },
        "private_rides": {
          "type": "string"
        },
        "public_rides": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "schema": {
      "type": "object",
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
