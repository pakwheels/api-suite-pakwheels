{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "New Car Make Catalogue",
  "type": "object",
  "required": [
    "error",
    "manufacturer_title",
    "active_models"
  ],
  "properties": {
    "error": {
      "type": "string"
    },
    "manufacturer_title": {
      "type": "string",
      "minLength": 1
    },
    "heading": {
      "type": "string"
    },
    "search_text": {
      "type": ["string", "null"]
    },
    "active_models": {
      "type": "object",
      "required": [
        "heading",
        "models"
      ],
      "properties": {
        "heading": {
          "type": "string",
          "minLength": 1
        },
        "models": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/modelSummary"
          }
        }
      },
      "additionalProperties": true
    },
    "other_models": {
      "type": "object",
      "properties": {
        "heading": {
          "type": "string"
        },
        "models": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/modelSummary"
          }
        }
      },
      "additionalProperties": true
    },
    "discontinued_models": {
      "type": ["array", "null"],
      "items": {
        "$ref": "#/definitions/modelSummary"
      }
    },
    "imported_models": {
      "type": ["array", "null"],
      "items": {
        "$ref": "#/definitions/modelSummary"
      }
    },
    "upcoming_models": {
      "type": ["array", "null"],
      "items": {
        "$ref": "#/definitions/modelSummary"
      }
    },
    "make_alternatives": {
      "type": "object",
      "properties": {
        "heading": {
          "type": "string"
        },
        "makes": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "title",
              "link"
            ],
            "properties": {
              "title": {
                "type": "string"
              },
              "logo": {
                "type": ["string", "null"]
              },
              "link": {
                "type": "string"
              }
            },
            "additionalProperties": true
          }
        }
      },
      "additionalProperties": true
    },
    "comparisons": {
      "type": "object",
      "properties": {
        "heading": {
          "type": "string"
        },
        "comparisons": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "url",
              "title"
            ],
            "properties": {
              "url": {
                "type": "string"
              },
              "title": {
                "type": "string"
              },
              "comparison_pictures_1": {
                "$ref": "#/definitions/displayPicture"
              },
              "comparison_pictures_2": {
                "$ref": "#/definitions/displayPicture"
              }
            },
            "additionalProperties": true
          }
        }
      },
      "additionalProperties": true
    },
    "videos": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "video"
        ],
        "properties": {
          "video": {
            "type": "object",
            "required": [
              "id",
              "title",
              "url"
            ],
            "properties": {
              "id": {
                "type": ["integer", "string"]
              },
              "title": {
                "type": "string"
              },
              "url": {
                "type": "string",
                "format": "uri"
              },
              "youtube_id": {
                "type": ["string", "null"]
              },
              "category": {
                "type": ["integer", "null"]
              }
            },
            "additionalProperties": true
          }
        },
        "additionalProperties": true
      }
    },
    "faqs": {
      "type": "object",
      "properties": {
        "h2": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "question",
              "answer"
            ],
            "properties": {
              "question": {
                "type": "string"
              },
              "answer": {
                "type": "string"
              }
            },
            "additionalProperties": true
          }
        }
      },
      "additionalProperties": true
    },
    "reviews": {
      "type": "object",
      "properties": {
        "h2": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": ["integer", "string"]
              },
              "title": {
                "type": "string"
              },
              "sub_title": {
                "type": ["string", "null"]
              },
              "detail_url": {
                "type": ["string", "null", "boolean"]
              },
              "overall_rating": {
                "type": ["number", "null"]
              }
            },
            "additionalProperties": true
          }
        }
      },
      "additionalProperties": true
    },
    "bread_crumbs": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "label"
        ],
        "properties": {
          "label": {
            "type": "string"
          },
          "url": {
            "type": ["string", "null"]
          }
        },
        "additionalProperties": true
      }
    },
    "car_dealers_by_city": {
      "type": "object",
      "properties": {
        "heading": {
          "type": "string"
        },
        "links": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "url",
              "link_text"
            ],
            "properties": {
              "url": {
                "type": "string"
              },
              "link_text": {
                "type": "string"
              },
              "tooltip": {
                "type": ["string", "null"]
              }
            },
            "additionalProperties": true
          }
        }
      },
      "additionalProperties": true
    },
    "similar_links": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "heading": {
            "type": "string"
          },
          "links": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "title",
                "url"
              ],
              "properties": {
                "title": {
                  "type": "string"
                },
                "url": {
                  "type": "string"
                },
                "h1": {
                  "type": ["string", "null"]
                }
              },
              "additionalProperties": true
            }
          }
        },
        "additionalProperties": true
      }
    },
    "custom_dimensions": {
      "type": "object",
      "required": [
        "user_type"
      ],
      "properties": {
        "user_type": {
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true,
  "definitions": {
    "displayPicture": {
      "type": "object",
      "properties": {
        "medium": {
          "type": ["string", "null"]
        },
        "original": {
          "type": ["string", "null"]
        },
        "thumbnail": {
          "type": ["string", "null"]
        },
        "resized": {
          "type": ["string", "null"]
        }
      },
      "additionalProperties": true
    },
    "modelSummary": {
      "type": "object",
      "required": [
        "title",
        "link"
      ],
      "properties": {
        "title": {
          "type": "string"
        },
        "price": {
          "type": ["string", "null"]
        },
        "schema_price": {
          "type": ["number", "null"]
        },
        "review_rating": {
          "type": ["number", "null"]
        },
        "reviewCount": {
          "type": ["number", "null"]
        },
        "link": {
          "type": "string"
        },
        "display_pic": {
          "$ref": "#/definitions/displayPicture"
        },
        "used_listing_link": {
          "type": ["string", "null"]
        },
        "used_listing_title": {
          "type": ["string", "null"]
        },
        "used_listing_link_text": {
          "type": ["string", "null"]
        }
      },
      "additionalProperties": true
    }
  }
}
